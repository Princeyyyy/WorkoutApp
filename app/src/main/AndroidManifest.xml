<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.workoutapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WorkoutApp">
        <activity android:name=".ui.ShouldersWorkoutActivity"></activity>
        <activity android:name=".ui.LegsWorkoutActivity" />
        <activity android:name=".ui.ChestWorkoutActivity" />
        <activity android:name=".ui.CalvesWorkoutActivity" />
        <activity android:name=".ui.BackWorkoutActivity" />
        <activity android:name=".ui.ArmsWorkoutActivity" />
        <activity android:name=".ui.AbsWorkoutActivity" />
        <activity android:name=".ui.WorkoutActivity" />
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>